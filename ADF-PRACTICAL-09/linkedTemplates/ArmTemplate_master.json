{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-PRACTICAL-09"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LK_SQL_PARQUET_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LK_SQL_PARQUET'"
		},
		"LS_SQL_Dataset_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_SQL_Dataset'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpracticaloutput.dfs.core.windows.net/"
		},
		"LK_SQL_PARQUET_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adfpracticaloutput.dfs.core.windows.net/"
		},
		"LS_SQL_Dataset_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "ANISH\\SQLEXPRESS"
		},
		"LS_SQL_Dataset_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "SqlLearnPractice"
		},
		"LS_SQL_Dataset_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ADF-PRACTICAL-09_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LK_SQL_PARQUET_accountKey": {
						"value": "[parameters('LK_SQL_PARQUET_accountKey')]"
					},
					"LS_SQL_Dataset_password": {
						"value": "[parameters('LS_SQL_Dataset_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LK_SQL_PARQUET_properties_typeProperties_url": {
						"value": "[parameters('LK_SQL_PARQUET_properties_typeProperties_url')]"
					},
					"LS_SQL_Dataset_properties_typeProperties_server": {
						"value": "[parameters('LS_SQL_Dataset_properties_typeProperties_server')]"
					},
					"LS_SQL_Dataset_properties_typeProperties_database": {
						"value": "[parameters('LS_SQL_Dataset_properties_typeProperties_database')]"
					},
					"LS_SQL_Dataset_properties_typeProperties_userName": {
						"value": "[parameters('LS_SQL_Dataset_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}